[Unit]
Description=Kubernetes Kube-Proxy Server
Documentation=https://github.com/GoogleCloudPlatform/kubernetes
After=network.target

[Service]
WorkingDirectory={{ kube_proxy_dir }}
ExecStart={{ kube_bin }}/kube-proxy \
  --config={{ kube_dir }}/kube-proxy-config.yml
Restart=always
RestartSec=5
LimitNOFILE=65536

[Install]
WantedBy=multi-user.target
